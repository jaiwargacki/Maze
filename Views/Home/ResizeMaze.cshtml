@model Maze
@{
    Layout = null;
}

<table id="Maze" class="hoverEMPTY">
    @for(int y = 0; y < Model.Height; y++) {
        <tr>
            @for(int x = 0; x < Model.Width; x++) {
                <td>
                    <div id="@(Model.GetSquare(x,y).ToString())" class="square square@(Model.GetSquare(x, y).Type)" valuex="@(x)" valuey="@(y)"></div>
                </td>
            }
        </tr>
    }
</table>